<Window x:Class="CourseWork.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseWork"
        mc:Ignorable="d"
        Title="Ввод" Height="264.313" Width="307.157" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="IntroText" HorizontalAlignment="Left" Height="42" Margin="10,10,0,0" TextWrapping="Wrap" Text="Введите не более 10 чисел в диапазоне от -9999 до 10000, разделенных запятыми" VerticalAlignment="Top" Width="279"/>
        <TextBox x:Name="InputText" HorizontalAlignment="Left" Height="132" Margin="10,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="279"/>
        <TextBlock x:Name="ErrorText" HorizontalAlignment="Left" Height="19" Margin="130,198,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="153" IsEnabled="False"/>
        <Button x:Name="Proceed" Content="Применить" HorizontalAlignment="Left" Height="34" Margin="10,189,0,0" VerticalAlignment="Top" Width="115" Click="Proceed_Click"/>

    </Grid>
</Window>
